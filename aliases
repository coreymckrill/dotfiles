# !/bin/bash

alias dotfiles="cd $DOTFILES_DIR"
alias dotfiles-refresh="source ~/.bash_profile"

# See https://github.com/iandunn/dotfiles/blob/a45ac99d07edeb5394cba6ea863508b4fee53592/.bash_aliases
# and functions
alias svnstat="svn stat | prune_svn_stat"

alias svnlog="svn log -l 5 ."
alias svnrevertall="svn revert --recursive ."
